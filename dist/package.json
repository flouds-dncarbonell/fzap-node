    },
    "repository": {
        "type": "git",
        "url": "https://github.com/flouds-dncarbonell/fzap-node.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
